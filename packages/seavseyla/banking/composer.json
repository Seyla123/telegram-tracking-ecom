{
    "name": "seav-seyla/banking",
    "description": "A Laravel package for banking.",
    "type": "laravel-package",
    "keywords": [
        "laravel",
        "package",
        "banking"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SeavSeyla\\Banking\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SeavSeyla\\Banking\\BankingServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0 || ^11.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0 || ^9.0"
    }
}
